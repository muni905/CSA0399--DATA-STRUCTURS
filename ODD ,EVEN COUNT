#include<stdio.h>
int main()
{
	int A[100],i,n,CO=0,CE=0;
	printf("Enter the number of elements:");
	scanf("%d",&n);
	printf("Enter the elements :\n");
	for (i=1;i<=n;i++)
	{
		scanf("%d",&A[i]);
	}
	for (i=1;i<=n;i++)
	{
		if (A[i]%2==0)
	{
		CE++;
	}
	else
	{
		CO++;
	}
	}
	printf("Even Count = %d\n",CE);
	printf("ODD Count = %d\n",CO);
	return 0;
}
